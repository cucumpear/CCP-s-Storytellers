    <listOrder>50</listOrder>
    <desiredPopulationMin>9</desiredPopulationMin>
    <desiredPopulationMax>25</desiredPopulationMax>
    <desiredPopulationCritical>55</desiredPopulationCritical>
    <comps>
      <li Class="StorytellerCompProperties_RandomMain">
        <minDaysPassed>1</minDaysPassed>
        <incidentMtb>0.55</incidentMtb>
        <maxThreatBigIntervalDays>6</maxThreatBigIntervalDays>
        <categoryWeights>
          <Misc>8</Misc>
          <OrbitalVisitor>2</OrbitalVisitor>
          <AllyArrival>1.4</AllyArrival>
          <ShipChunkDrop>0.02</ShipChunkDrop>
          <ThreatSmall>1.75</ThreatSmall>
          <ThreatBig>3</ThreatBig>
